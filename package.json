{
  "name": "rollup-plugin-jscc",
  "version": "1.0.0",
  "description": "Conditional comments and variable replacement for rollup, based on jscc",
  "main": "./index.js",
  "types": "./index.d.ts",
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "lint": "eslint --ext js src test",
    "pretest": "rollup -c",
    "test": "nyc mocha",
    "build": "rm -rf dist/* && rollup -c",
    "prepublishOnly": "npm run lint && npm test"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "javascript",
    "typescript",
    "es2015",
    "es6",
    "modules",
    "compilation",
    "preproccesor",
    "jspreproc",
    "jscc"
  ],
  "author": {
    "name": "aMarCruz",
    "email": "amarcruzbox-git@yahoo.com",
    "url": "https://amarcruz.github.io"
  },
  "license": "MIT",
  "bugs": "https://github.com/aMarCruz/rollup-plugin-jscc/issues",
  "homepage": "https://github.com/aMarCruz/rollup-plugin-jscc",
  "repository": {
    "type": "git",
    "url": "https://github.com/aMarCruz/rollup-plugin-jscc"
  },
  "dependencies": {
    "@jsbits/get-package-version": "^1.0.1",
    "jscc": "^1.1.0",
    "rollup-pluginutils": "^1.5.2"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rollup": "^0.67.3",
    "rollup-plugin-node-resolve": "^3.4.0"
  }
