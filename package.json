{
  "name": "rollup-plugin-jscc",
  "version": "0.1.0-beta.1",
  "description": "Conditional comments and code cleanup for rollup, based on jspreproc",
  "main": "dist/rollup-plugin-jscc.cjs.js",
  "jsnext:main": "dist/rollup-plugin-jscc.es.js",
  "scripts": {
    "test": "npm run build-cjs && mocha test/run.js",
    "pretest": "npm build",
    "build": "npm run build-cjs && npm run build-es6 && npm run build-pre",
    "build-cjs": "rollup -c -f cjs -o dist/rollup-plugin-jscc.cjs.js",
    "build-es6": "rollup -c -f es -o dist/rollup-plugin-jscc.es.js",
    "build-pre": "rollup -c -f cjs --environment LEGACY=1 -o dist/legacy/rollup-plugin-jscc.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aMarCruz/rollup-plugin-jscc"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "javascript",
    "es2015",
    "es6",
    "modules",
    "jspreproc",
    "conditional comments"
  ],
  "author": "amarcruz <amarcruzbox-git@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aMarCruz/rollup-plugin-jscc/issues"
  },
  "homepage": "https://github.com/aMarCruz/rollup-plugin-jscc",
  "dependencies": {
    "magic-string": "^0.16.0",
    "rollup-pluginutils": "^1.5.2"
  },
  "devDependencies": {
    "eslint": "^3.4.0",
    "expect": "^1.20.2",
    "mocha": "^3.0.2",
    "rollup": "^0.34.11",
    "rollup-plugin-buble": "^0.13.0"
  }
}
