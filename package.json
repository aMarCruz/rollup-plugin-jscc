{
  "name": "rollup-plugin-jscc",
  "version": "0.2.0",
  "description": "Conditional comments and code cleanup for rollup, based on jspreproc",
  "main": "dist/rollup-plugin-jscc.cjs.js",
  "module": "dist/rollup-plugin-jscc.es.js",
  "jsnext:main": "dist/rollup-plugin-jscc.es.js",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "test": "node-version-gte-4 && mocha || mocha --compilers js:./buble-register",
    "lint": "node-version-gte-4 && eslint src test/index.js || node-version-lt-4",
    "pretest": "npm run build:cjs",
    "build": "rm -rf dist/* && npm run build:cjs && npm run build:es6",
    "build:cjs": "rollup -c -f cjs -o dist/rollup-plugin-jscc.cjs.js",
    "build:es6": "rollup -c -f es -o dist/rollup-plugin-jscc.es.js",
    "prepublish": "npm run lint && npm run build:es6 && npm test"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aMarCruz/rollup-plugin-jscc"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "javascript",
    "es2015",
    "es6",
    "modules",
    "jspreproc",
    "conditional comments"
  ],
  "author": {
    "name": "aMarCruz",
    "email": "amarcruzbox-git@yahoo.com",
    "url": "https://github.com/aMarCruz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aMarCruz/rollup-plugin-jscc/issues"
  },
  "homepage": "https://github.com/aMarCruz/rollup-plugin-jscc",
  "dependencies": {
    "magic-string": "^0.16.0",
    "rollup-pluginutils": "^1.5.2"
  },
  "devDependencies": {
    "buble": "^0.14.0",
    "eslint": "^3.6.0",
    "expect": "^1.20.2",
    "mocha": "^3.0.2",
    "node-version-check": "^2.1.1",
    "rollup": "^0.36.0",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-node-resolve": "^2.0.0"
  }
}
